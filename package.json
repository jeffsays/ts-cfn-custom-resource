{
  "name": "@jeffsays/cfn-custom-resource-ts",
  "version": "0.0.0",
  "description": "Node.js module providing utility functions for AWS Cloudformation Custom Resources.",
  "keywords": [
    "aws",
    "cloudformation",
    "cfn",
    "custom",
    "custom resource",
    "response"
  ],
  "homepage": "https://github.com/jeffsays/cfn-custom-resource",
  "bugs": {
    "url": "https://github.com/jeffsays/cfn-custom-resource/issues"
  },
  "repository": "git@github.com:jeffsays/cfn-custom-resource.git",
  "license": "MIT",
  "author": "Jeff Beck",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run build:license-checker && tsc",
    "build:license-checker": "license-checker --csv --out var/licenses.csv",
    "clean": "shx rm -rf lib",
    "doc": "jsdoc2md src/index.ts > TEMP.MD",
    "prepare": "husky install",
    "prettier:check": "node --import tsx node_modules/prettier/bin/prettier.cjs . --check",
    "prettier:list-different": "node --import tsx node_modules/prettier/bin/prettier.cjs . --list-different",
    "prettier:write": "node --import tsx node_modules/prettier/bin/prettier.cjs . --write",
    "test": "jest"
  },
  "lint-staged": {
    "*": "yarn run prettier:write"
  },
  "prettier": "@jeffsays/prettier",
  "devDependencies": {
    "@jeffsays/prettier": "^0.0.14",
    "@types/jest": "^29.5.14",
    "@types/license-checker": "^25",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^9.1.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.5.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.3.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.0.0",
  "ts-standard": {
    "ignore": [
      ".idea",
      ".yarn",
      "node_modules",
      "lib",
      "var"
    ],
    "project": "./tsconfig.json"
  }
}
